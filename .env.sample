# General Environment Settings
ENV=development  # Environment setting (development, production, etc.)
SITE_URL=http://127.0.0.1:3000  # The base URL of the site

# Supabase Configuration
SUPABASE_URL=https://xyzcompany.supabase.co  # Supabase project URL
SUPABASE_KEY=your-supabase-key  # Supabase anonymous or service key
SUPABASE_API_KEY=your-supabase-api-key  # Additional API key if required
SUPABASE_SERVICE_ROLE_KEY=your-supabase-service-role-key  # Service role key
SUPABASE_JWT_SECRET=your-supabase-jwt-secret  # JWT secret

# Supabase Database Settings
SUPABASE_DB_PASSWORD=your-db-password  # Database password
SUPABASE_REGION=us-west-1  # Supabase region (e.g., us-east-1)
SUPABASE_PROJECT_ID=your-project-id  # Supabase project ID
SUPABASE_USER=your-db-user  # Database user
SUPABASE_HOST=your-db-host.supabase.co  # Database host

# Email Configuration
EMAIL_USER=your-email-username  # Email service username
EMAIL_HOST=smtp.your-email.com  # Email server host
EMAIL_PORT=587  # Email server port
EMAIL_PASS=your-email-password  # Email service password
EMAIL_ADMIN=admin@example.com  # Administrator email address

# Analytics Configuration
GOOGLE_ANALYTICS_ID=your-google-analytics-id  # Google Analytics ID
